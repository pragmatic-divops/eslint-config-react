{
  "name": "@pragmatic-divops/eslint-config-react",
  "description": "Shareable config for ESLint for React components",
  "license": "MIT",
  "version": "0.0.0-semantically-released",
  "files": [
    "example.js",
    "index.js"
  ],
  "publishConfig": {
    "access": "public"
  },
  "runkitExampleFilename": "./example.js",
  "repository": "pragmatic-divops/eslint-config-react",
  "bugs": "https://github.com/pragmatic-divops/eslint-config-react/issues",
  "homepage": "https://npm.im/@pragmatic-divops/eslint-config-react",
  "author": "Matt Travi <npm@travi.org> (https://matt.travi.org)",
  "scripts": {
    "test": "npm-run-all --print-label --parallel lint:*",
    "lint:lockfile": "lockfile-lint",
    "lint:md": "remark . --frail",
    "generate:md": "remark . --output",
    "lint:sensitive": "ban",
    "lint:js": "eslint .",
    "lint:peer": "npm ls >/dev/null",
    "prepare": "husky install"
  },
  "dependencies": {
    "@form8ion/eslint-config-react": "1.16.2"
  },
  "devDependencies": {
    "@form8ion/babel-preset": "1.6.97",
    "@pragmatic-divops/commitlint-config": "1.0.24",
    "@pragmatic-divops/eslint-config": "1.0.49",
    "@pragmatic-divops/remark-preset": "3.0.1",
    "ban-sensitive-files": "1.9.18",
    "cz-conventional-changelog": "3.3.0",
    "husky": "8.0.1",
    "lockfile-lint": "4.8.0",
    "npm-run-all": "4.1.5",
    "remark-cli": "11.0.0",
    "remark-toc": "8.0.1",
    "remark-usage": "10.0.1"
  }
}
